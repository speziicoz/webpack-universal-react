{
  "name": "webpack-universal-react",
  "version": "1.0.0",
  "description": "React on the Server for Beginners: Build a Universal React and Node App",
  "author": "Champ N. Jaikaew",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": "7.6.0",
    "npm": "4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/speziicoz/webpack-universal-react.git"
  },
  "scripts": {
    "postinstall": "npm run build",
    "build": "npm run build-client && npm run build-server",
    "build-client": "rimraf ./public/* && webpack -p --colors --profile --config ./webpack/config.client.js",
    "build-server": "rimraf ./build/* && webpack -p --colors --profile --config ./webpack/config.server.js",
    "start": "node build/server.js",
    "start-dev": "node src/server/development.js",
    "dev": "webpack-dev-server"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "ejs": "^2.5.6",
    "ejs-html-loader": "^3.0.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "fast-sass-loader": "^1.2.5",
    "file-loader": "^0.11.2",
    "history": "^3.3.0",
    "html-webpack-plugin": "^2.29.0",
    "image-webpack-loader": "^3.3.1",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-css-modules": "^4.5.2",
    "react-dom": "^15.6.1",
    "react-iscroll": "^2.0.3",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "rodal": "^1.5.4",
    "serve-favicon": "^2.4.3",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "babel-register": "^6.24.1",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
